<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="PythonHandler" path="*.py" verb="*" modules="FastCGIModule" scriptProcessor="%Python37%\python.exe|%Python37%\python.exe" resourceType="Unspecified" />
        </handlers>
    </system.webServer>
</configuration>
